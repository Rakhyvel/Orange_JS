module Board
    struct Board(int array array grid)
    const int BOARDSIZE = 8

    Board create()
        Board retval = Board(int array array[BOARDSIZE])
        int i = 0
        while i < BOARDSIZE
            retval.grid[i] = int array[BOARDSIZE]
            i = i + 1
        end
        return retval
    end
end